{
  "presets" : [
    "react",
    [
      "env", {
        "targets": {
          "node": "6.10"
        }
      }
    ]
  ],
  "env" : {
    "development": {
      "plugins": [[
          "react-transform", {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": ["react"],
                "locals": ["module"]
              }
            ]
          }
      ]]
    }
  }
}
